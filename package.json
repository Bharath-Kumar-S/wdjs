{
  "name": "WDIO-with-mocha",
  "version": "1.0.0",
  "description": "WebDriver-v5 bindings for Node.js with Mocha",
  "main": "index.js",
  "scripts": {
    "node-inspector": "node-inspector --debug-port 5859 --no-preload",
    "test": "node node_modules/.bin/wdio ./test/config/suite.mocha.conf.js",
    "allure-report": "node_modules/.bin/allure generate ./test/reports/allure-results && allure open",
    "junit-report": "node_modules/.bin/junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html"
  },
  "keywords": [
    "web",
    "testing",
    "webdriverjs",
    "webdriverio",
    "automation",
    "mocha"
  ],
  "author": "Bharath Kumar S <bharathkumarkarthick@gmail.com> 9003045843",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/expect": "^1.20.3",
    "@wdio/allure-reporter": "^5.3.4",
    "@wdio/browserstack-service": "^5.3.4",
    "@wdio/dot-reporter": "^5.2.3",
    "@wdio/junit-reporter": "^5.3.1",
    "@wdio/sauce-service": "^5.3.2",
    "@wdio/selenium-standalone-service": "^5.2.2",
    "@wdio/spec-reporter": "^5.2.3",
    "@wdio/sync": "^5.3.2",
    "allure-commandline": "^2.13.0",
    "assert": "^1.4.1",
    "junit-viewer": "^4.11.1",
    "lodash": "^4.17.4",
    "node-excel-to-json": "0.0.3",
    "underscore": "^1.8.3",
    "webdriverio": "^5.16.7"
  },
  "devDependencies": {
    "@wdio/cli": "^5.3.5",
    "@wdio/local-runner": "^5.3.5",
    "@wdio/mocha-framework": "^5.3.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "grunt": "^1.5.3",
    "grunt-cli": "^1.3.2",
    "grunt-mocha": "^1.2.0",
    "grunt-webdriver": "^3.0.0",
    "leche": "^2.3.0",
    "selenium-standalone": "^6.15.4",
    "wdio-image-comparison-service": "^1.8.0",
    "wdio-timeline-reporter": "^5.1.1",
    "wdio-video-reporter": "^1.4.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
